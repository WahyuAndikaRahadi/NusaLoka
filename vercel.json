{
  "rewrites": [
  {
    "source": "/api/:path*",
    "destination": "/api/index.js"
  },
  {
    "source": "/(.*)",
    "destination": "/index.html"
  }
],

  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist"
      }
    }
  ]
}